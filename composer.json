{
    "name": "apiship/yandex-market-php-client",
    "description": "Integration (SDK) with Yandex Market API",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://apiship.ru",
    "authors": [
        {
          "name": "ApiShip",
          "email": "info@apiship.ru"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "YandexMarketApi\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "YandexMarketApi\\Test\\" : "test/" }
    }
}
